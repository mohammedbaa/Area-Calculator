public class AreaCalculator {
    public static double area(double radius){
        if (radius<0){
            return -1.0;
        }
        else{
            double A=radius*radius*Math.PI;
            return A;

        }

    }
    public static double area(double x,double y){
        if (x<0 || y<0){
            return -1.0;
        }
        else {
            double rec=x*y;
            return rec;

        }
    }
     
}
